{
  "name": "walrus-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "copy-deps": "copy-newer --cwd node_modules ** ./dist/node_modules",
    "dev": "ts-node ./server.ts",
    "start": "node ./bin/www",
    "preserve": "npm run build",
    "s": "npm run serve",
    "serve": "concurrently \"npm run build.w\" \"nodemon --watch ./dist/server ./dist/server/server.js\"",
    "lint": "tslint \"src/**/*.ts\"",
    "prebuild": "npm install",
    "b": "npm run build.w",
    "build": "npm run copy-deps && tsc",
    "build.w": "npm run copy-deps && tsc -w"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "raspi-1wire-temp": "^2.0.0",
    "rpio": "^2.4.2",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/ws": "^7.4.1",
    "copy-newer": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
